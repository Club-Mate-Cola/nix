{
	"enabled": 1,
	"hidden": false,
	"description": "master",
	"nixexprinput": "src",
	"nixexprpath": "release.nix",
	"checkinterval": 1800,
	"schedulingshares": 40,
	"enableemail": false,
	"emailoverride": "",
	"keepnr": 3,
	"inputs": {
  	"src": { "type": "git", "value": "git://github.com/kloenk/nix.git master", "emailresponsible": false },
  	"nixpkgs": { "type": "git", "value": "git://github.com/NixOS/nixpkgs.git master", "emailresponsible": false },
  	"krops" : { "type": "git", "value": "git://github.com/nyantec/krops master", "emailresponsible": false },
  	"home-manager": { "type": "git", "value": "git://github.com/rycee/home-manager.git master", "emailresponsible": false },
  	"nixos-mailserver": { "type": "git", "value": "https://gitlab.com/simple-nixos-mailserver/nixos-mailserver master", "emailresponsible": false },
		"secrets" : { "type": "path", "value": "/var/empty", "emailresponsible" : false }
	}
}
